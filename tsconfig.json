{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "models/*": ["src/models/*"],
            "game/*": ["src/models/game/*"],
            "event/*": ["src/models/events/*"],
            "components/*": ["src/components/*"],
            "utils/*": ["src/utils/*"],
        },
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    }
}